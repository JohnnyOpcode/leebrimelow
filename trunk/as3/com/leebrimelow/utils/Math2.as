package com.leebrimelow.utils {	/*	 * A collection of static methods for common math operations.	*/		public class Math2 	{		public static function randomFloat(min:Number,max:Number=NaN):Number 		{			if (isNaN(max)) 			{ 				max = min; 				min = 0; 			}			return Math.random() * (max - min) + min;		}				public static function random(min:Number,max:Number=NaN):int 		{			if (isNaN(max)) 			{ 				max = min; 				min = 0; 			}			return Math.floor(randomFloat(min, max));		}	}}